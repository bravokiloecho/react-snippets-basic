{
	"name": "react-snippets",
	"description": "Code snippets for Reactjs development in ES6 syntax",
	"version": "1.0.0",
	"displayName": "Reactjs code snippets",
	"publisher": "bravokiloecho",
	"icon": "images/react.png",
	"galleryBanner": {
		"theme": "light"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/xabikos/vscode-react"
	},
	"engines": {
		"vscode": "0.10.x"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "typescriptreact",
				"path": "./snippets/utils-global.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/utils-global.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/javascript/components.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/typescript/components.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/typescript/utils.json"
			}
		]
	}
}